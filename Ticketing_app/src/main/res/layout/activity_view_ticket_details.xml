<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/cardview"
    android:layout_width="match_parent"
    android:background="#FFCCBC"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            android:id="@+id/toolbar"
            layout="@layout/toolbar"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_tic_no"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:layout_weight="1"
                android:text="TICKET NO :"/>

            <TextView
                android:id="@+id/tic_no"
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_marginRight="@dimen/dp10"
                android:layout_weight="0.5"
                android:text="TextView"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_tic_subdiv_code"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:text="SUBDIV CODE :"/>

            <TextView
                android:id="@+id/tic_subdiv_code"
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_marginRight="10dp"
                android:layout_weight="0.5"
                android:text="TextView"/>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="@dimen/dp4"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_tic_file"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:text="UPLOAD FILE :"/>

            <TextView
                android:id="@+id/tic_file"
                android:layout_width="120dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_weight="0.5"
                android:textColor="#006400"
                android:text="TextView" />

            <TextView
                android:id="@+id/download"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:text="Download"
                android:textStyle="bold"
                android:textAlignment="center"
                android:textColor="#CD212A"
                android:background="@drawable/borders"
                android:layout_marginRight="10dp"
                android:layout_marginTop="@dimen/dp10"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="@dimen/dp4"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_tic_gen_by"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:text="GENERATED BY :"/>

            <TextView
                android:id="@+id/tic_gen_by"
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_marginRight="10dp"
                android:layout_weight="0.5"
                android:text="TextView"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="@dimen/dp4"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_tic_gen_on"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:text="GENERATED ON :"/>

            <TextView
                android:id="@+id/tic_gen_on"
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_marginRight="10dp"
                android:layout_weight="0.5"
                android:text="TextView"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/tic_close_layout"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="@dimen/dp4"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_tic_close"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:text="CLOSED ON :"/>

            <TextView
                android:id="@+id/tic_close"
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_marginRight="10dp"
                android:layout_weight="0.5"
                android:text="TextView"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="@dimen/dp4"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_tic_priority"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:layout_weight="1"
                android:text="PRIORITY :"/>

            <TextView
                android:id="@+id/tic_priority"
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_marginRight="10dp"
                android:layout_weight="0.5"
                android:text="TextView"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="@dimen/dp4"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_tic_severity"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:text="SEVERITY :"/>

            <TextView
                android:id="@+id/tic_severity"
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_marginRight="10dp"
                android:layout_weight="0.5"
                android:text="TextView"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="@dimen/dp4"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_tic_assign_to"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:text="ASSIGN TO :"/>

            <TextView
                android:id="@+id/tic_assign_to"
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_marginRight="10dp"
                android:layout_weight="0.5"
                android:text="TextView"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="@dimen/dp4"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_tic_hescom_tvd"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:text="DEPARTMENT :"/>

            <TextView
                android:id="@+id/tic_hescom_tvd"
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_marginRight="10dp"
                android:layout_weight="0.5"
                android:text="TextView"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="@dimen/dp4"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_tic_status"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:text="STATUS :"/>

            <TextView
                android:id="@+id/tic_status"
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_marginRight="10dp"
                android:layout_weight="0.5"
                android:text="TextView"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="@dimen/dp4"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_tic_title"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:text="TITLE :"/>

            <TextView
                android:id="@+id/tic_title"
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_marginRight="10dp"
                android:layout_weight="0.5"
                android:text="TextView"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="@dimen/dp4"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_tic_desc"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:text="DESCRIPTION :"/>

            <TextView
                android:id="@+id/tic_desc"
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_marginRight="10dp"
                android:layout_weight="0.5"
                android:text="TextView"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="@dimen/dp4"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txt_tic_narr"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:text="NARRATION :"/>

            <TextView
                android:id="@+id/tic_narr"
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_marginRight="10dp"
                android:layout_weight="0.5"
                android:text="TextView"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/tic_cmnt"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="@dimen/dp4"
            android:orientation="horizontal">

            <TextView
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/dp16"
                android:layout_marginTop="@dimen/dp10"
                android:text="COMMENT :"/>

            <TextView
                android:id="@+id/txt_tic_comm"
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp10"
                android:layout_marginRight="10dp"
                android:layout_weight="0.5"
                android:text="TextView"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="@dimen/dp10"
            android:layout_marginTop="20dp">

            <Button
                android:id="@+id/btn_edit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/black"
                android:backgroundTint="#84C9FD"
                android:textStyle="bold"
                android:layout_marginLeft="@dimen/dp5"
                android:text="Edit"/>

            <Button
                android:id="@+id/btn_details"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/black"
                android:backgroundTint="#84C9FD"
                android:textStyle="bold"
                android:layout_marginRight="@dimen/dp5"
                android:text="Details"/>
        </LinearLayout>
    </LinearLayout>
</ScrollView>